{
  "presets": [
    ["@babel/preset-react"],

    [
      "@babel/preset-env",
      {
        "loose": true,
        "useBuiltIns": false,
        "modules": false,
        "shippedProposals": true,
        "targets": "last 5 versions"
      }
    ]
  ],
  "plugins": [["@babel/plugin-proposal-class-properties"]]
}
